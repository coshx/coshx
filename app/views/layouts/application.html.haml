!!! 5
%html
  %head
    %title= "Coshx Labs #{yield(:title)}"
    %meta{:name => "google-site-verification", :content => "GG9HzTQhQZszRRYo-W_el_Dh1LQs-3SQOuCzZS88FvI"}
    = render "layouts/fonts"
    = stylesheet_link_tag "application"
    = javascript_include_tag "application"
    %meta(content="width=device-width;maximum-scale=1.0;minimum-scale=1.0;" name="viewport")/
    = csrf_meta_tags
    = render "layouts/google_analytics"
    = render "layouts/disqus_dev_mode" if Rails.env.development? || Rails.env.test?
    = auto_discovery_link_tag :atom, "/feed"
    = favicon_link_tag
  %body
    .save_button
      Save!
    %div.container
      %div#header.row
        - if admin_signed_in?
          = render "layouts/admin_menu"
        = render "layouts/header"
      %div#content.row
        = yield
    = render "layouts/footer"

    

